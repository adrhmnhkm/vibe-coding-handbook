# Contoh file .env untuk proyek fullstack

# ========== BACKEND ==========
# URL database (contoh: Postgres, MySQL, Mongo)
DATABASE_URL=

# Environment (development / production)
NODE_ENV=development

# Port server backend
PORT=3001

# JWT
JWT_SECRET=
JWT_EXPIRES_IN=7d

# CORS (boleh lebih dari satu, pisahkan dengan koma)
CORS_ORIGIN=http://localhost:3000

# ========== FRONTEND ==========
# Base URL API yang dipanggil frontend
NEXT_PUBLIC_API_BASE_URL=http://localhost:3001

# ========== EMAIL / NOTIFIKASI ==========
SMTP_HOST=
SMTP_PORT=
SMTP_USER=
SMTP_PASS=
SMTP_FROM_NAME=
SMTP_FROM_EMAIL=

# ========== LAYANAN PIHAK KETIGA ==========
# Contoh: Stripe, Midtrans, Firebase, Supabase, dsb.
STRIPE_SECRET_KEY=
MIDTRANS_SERVER_KEY=
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
